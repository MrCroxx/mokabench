{
    "editor.rulers": [85],
    "rust-analyzer.cargo.features": ["hashlink", "mini-moka", "quick_cache", "stretto"],
    "rust-analyzer.server.extraEnv": {
        "CARGO_TARGET_DIR": "target/ra"
    },
    "cSpell.words": [
        "clippy",
        "Dharmendra",
        "hashbrown",
        "Hasher",
        "hashlink",
        "itertools",
        "Modha",
        "moka",
        "mokabench",
        "oltp",
        "thiserror",
        "Toolchain",
        "unsync",
        "Zstandard"
    ],
    "files.watcherExclude": {
        "**/target": true
    }
}
